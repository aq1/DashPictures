[Unit]
Description=uWSGI app
After=syslog.target

[Service]
#ExecStart=/_projects/dash_pictures/.env/bin/uwsgi --ini /_projects/dash_pictures/project/configs/uwsgi.ini
ExecStart=/_projects/dash_pictures/.env/bin/python /_projects/dash_pictures/manage.py runserver 0.0.0.0:80
User=nginx
Group=nginx
Restart=on-failure
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all
